{
    "name": "@lanco-incidents/func",
    "scripts": {
        "azurite": "docker compose -f ../docker-compose.yml up -d",
        "func": "pnpm azurite &&func start --csharp",
        "dev": "concurrently --names \"Azurite,Functions\" -c \"bgBlue.bold,bgMagenta.bold\" \"pnpm run azurite\" \"pnpm run func\"",
        "dev:vercel": "pnpm dev",
        "serve": "pnpm dev",
        "build": "dotnet build -c Release",
        "restore": "dotnet clean && dotnet restore"
    },
    "lint-staged": {
        "*.{cs}": [
            "dotnet csharpier"
        ]
    },
    "volta": {
        "extends": "../../package.json"
    },
    "devDependencies": {
        "concurrently": "^7.6.0"
    }
}
