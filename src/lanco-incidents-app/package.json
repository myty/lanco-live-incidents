{
    "name": "lanco-incidents-app",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "coverage": "vitest run --coverage",
        "coverage-report": "vitest run --coverage && codecov --disable=gcov",
        "lint": "tsc --noEmit && eslint . --ext .js,.jsx,.ts,.tsx --fix && rome check --apply",
        "test": "vitest"
    },
    "dependencies": {
        "@tailwindcss/forms": "^0.5.3",
        "axios": "^1.1.2",
        "geolib": "^3.3.3",
        "google-map-react": "^2.2.0",
        "jotai": "^1.8.5",
        "lodash": "^4.17.21",
        "luxon": "^3.0.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.4.2",
        "simple-immutable-record": "^0.1.4",
        "workbox-build": "^6.5.4",
        "workbox-window": "^6.5.4"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-typescript": "^7.18.6",
        "@ehutch79/vite-eslint": "^0.0.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@types/google-map-react": "^2.1.7",
        "@types/lodash": "^4.14.186",
        "@types/luxon": "^2.4.0",
        "@types/node": "^17.0.45",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/react-router-dom": "^5.3.3",
        "@types/workbox-build": "^5.0.1",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vitejs/plugin-react": "^2.1.0",
        "autoprefixer": "^10.4.12",
        "babel-loader": "^8.2.5",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^6.0.0",
        "eslint-plugin-flowtype": "^5.10.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jsdom": "latest",
        "postcss": "^8.4.18",
        "prettier": "^2.7.1",
        "rome": "^10.0.1",
        "tailwindcss": "^3.1.8",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4",
        "vite": "^3.1.8",
        "vite-plugin-pwa": "^0.13.1",
        "vite-tsconfig-paths": "^3.5.1",
        "vitest": "^0.24.1"
    },
    "babel": {
        "presets": [
            [
                "@babel/preset-env",
                {
                    "targets": {
                        "node": "current"
                    }
                }
            ],
            "@babel/preset-typescript"
        ]
    },
    "lint-staged": {
        "*.{ts,js}": ["eslint --cache --fix", "rome check --apply", "rome format --write"],
        "*.{tsx,css,md}": ["eslint --cache --fix", "prettier --write"]
    },
    "volta": {
        "extends": "../../package.json"
    }
}