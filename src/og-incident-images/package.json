{
    "name": "@lanco-incidents/og-images",
    "scripts": {
        "azurite": "docker compose up -d",
        "debug:test:integration": "pnpm azurite && deno test --inspect-wait --allow-env --allow-sys --allow-net --allow-read=./local.settings.json -- UseDevelopmentStorage=true;",
        "test:integration": "pnpm azurite && deno test --watch --allow-env --allow-sys --allow-net --allow-read=./local.settings.json -- UseDevelopmentStorage=true;",
        "dev": "func host start",
        "dev:vercel": "pnpm dev"
    },
    "lint-staged": {
        "*.{ts,tsx,js,json}": [
            "deno lint",
            "deno fmt"
        ]
    },
    "volta": {
        "extends": "../../package.json"
    },
    "devDependencies": {
        "dotenv-cli": "^6.0.0"
    }
}
