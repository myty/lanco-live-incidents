{
    "name": "lanco-live-incidents",
    "version": "0.1.0",
    "author": "Michael Tyson <michaeltyson@outlook.com>",
    "license": "MIT",
    "scripts": {
        "dev": "yarn --cwd src/lanco-incidents-app dev",
