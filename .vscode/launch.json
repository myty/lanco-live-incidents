    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Edge",
            "request": "launch",
            "type": "msedge",
            "url": "http://localhost:5173",
            "webRoot": "${workspaceFolder}/src/lanco-incidents-app"
        },
        {
            "name": "Attach to .NET Functions",
            "type": "coreclr",
            "justMyCode": false,
            "request": "attach",
            "processId": "${command:azureFunctions.pickProcess}"
        },
        {
            "name": "Deno Integration Test",
            "type": "node",
            "request": "attach",
            "cwd": "${workspaceFolder}/src/og-incident-images",
            "port": 9229,
        }
    ]
}
