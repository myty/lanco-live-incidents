{
    "liveFrame.url": "http://localhost:3000",
    "liveFrame.title": "Local Development",
    "liveFrame.pane": "Beside",
    "files.exclude": {
        "**/dist/**": true,
        "**/node_modules/**": true,
        "**/tsconfig.tsbuildinfo": true
    },
    "eslint.workingDirectories": [
        { "directory": "src/lanco-incidents-app", "changeProcessCWD": true }
    ],
    "azureFunctions.projectSubpath": "src/lanco-incidents-func",
    "azureFunctions.deploySubpath": "src/lanco-incidents-func/bin/Release/net6.0/publish",
    "azureFunctions.projectLanguage": "C#",
    "azureFunctions.projectRuntime": "~4",
    "debug.internalConsoleOptions": "neverOpen",
    "azureFunctions.preDeployTask": "publish (functions)",
    "[csharp]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "csharpier.csharpier-vscode"
    }
}
