{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "serve": {
      "dependsOn": [
        "^build",
        "build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lanco-incidents-app#test": {
      "persistent": true,
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "inputs": [
        "src/**/*.tsx",
        "src/**/*.ts",
        "test/**/*.ts",
        "test/**/*.tsx"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": [
        "build",
        "test",
        "lint"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:frontend": {
      "cache": false,
      "persistent": true
    },
    "azurite": {
      "cache": false,
      "persistent": true
    },
    "lanco-incidents-func#dev": {
      "cache": false,
      "persistent": true,
      "outputs": [
        "Debug/**"
      ]
    },
    "lanco-incidents-func#build": {
      "outputs": [
        "Release/**"
      ]
    }
  }
}
